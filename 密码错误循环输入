#include<stdio.h>
#include<string.h>
int main(void)
{
 char mima[20];
 int i =0;
 for(i = 1;i <= 5;i++)
 {
  printf("请输入密码:\n");
  scanf("%s",&mima);
  if(strcmp(mima,"6666") == 0)
  {
   printf("密码正确\n");
   printf("登录成功\n");
   break;
  }
  if(i == 5)
  {
    printf("密码输错5次，请1分钟后再尝试\n");
  }
  else{
   printf("密码错误，请重新输入\n");
  }
    }
  
return 0;
}
