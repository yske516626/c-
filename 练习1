#include<stdio.h>
#include<string.h>
int main(void)
{
	int ret;
	int mima = 0;
	
	printf("请输入四位密码：\n");
	scanf("%d",&mima);
	printf("是否确认（Y/N）:\n");
	getchar();
	ret = getchar();
	if ('Y' == ret)
	{
		
		if ( 6666 == mima)
		{
			printf("密码正确\n");
		}
		else {
			printf("密码错误\n");
		}
	}
	else {
		printf("放弃输入\n");
	}

	return 0;
}
